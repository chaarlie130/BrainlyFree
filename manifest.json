{
    "name": "Brainly Helper",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Brainly Answer Scraper",
    "content_scripts": [
        {
            "matches": ["https://brainly.com/*"],
            "js": ["init.js"]
        }
    ],
    "icons": {                   
        "19": "icon19.png",
        "38": "icon38.png",
        "48": "icon48.png",
        "128": "icon128.png"  
    },    
    "browser_action": {
        "default_icon": {                   
            "19": "icon19.png",
            "38": "icon38.png",
            "48": "icon48.png",
            "128": "icon128.png"        
        },
        "default_popup": "popup.html"
    }
}